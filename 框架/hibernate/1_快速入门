			/SimpleGOD/src/top/trial/hibernate/HibernateTestBaseUtil.java	--集成一些公共方法
			/SimpleGOD/src/top/trial/hibernate/TestForHibernateCRUD.java	--测试CRUD
			/SimpleGOD/src/top/trial/hibernate/GameEntity.hbm.xml			--Entity对应的配置文件
